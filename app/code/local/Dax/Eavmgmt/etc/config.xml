<?xml version="1.0" ?>
<config>
    <modules>
        <Dax_Eavmgmt>    
            <version>1.0.0</version>
        </Dax_Eavmgmt>
    </modules>
    <frontend>
        <layout>
            <updates>
                <Eavmgmt module="Dax_Eavmgmt">
                    <file>Dax_Eavmgmt.xml</file>
                </Eavmgmt>
            </updates>
        </layout>
    </frontend>
    <global>
        <resources>
            <eavmgmt_setup>
                <setup>
                    <module>Dax_Eavmgmt</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </eavmgmt_setup>
            <eavmgmt_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </eavmgmt_write>
            <eavmgmt_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </eavmgmt_read>
        </resources>
        <models>
            <eavmgmt>
                <class>Dax_Eavmgmt_Model</class>
                <resourceModel>eavmgmt_resource</resourceModel>
            </eavmgmt>
            <eavmgmt_resource>
                <class>Dax_Eavmgmt_Model_Resource</class>
                <deprecatedNode>eavmgmt_mysql4</deprecatedNode>
                <entities>
                    <eavmgmt>
                        <table>eav_attribute</table>
                    </eavmgmt>
                </entities>
            </eavmgmt_resource>
        </models>
        <blocks>
            <eavmgmt>
                <class>Dax_Eavmgmt_Block</class>
            </eavmgmt>
        </blocks>
        <helpers>
           <eavmgmt>
                  <class>Dax_eavmgmt_Helper</class>
            </eavmgmt>
        </helpers>
    </global>
    <admin>
        <routers>
            <Eavmgmt>
                <use>admin</use>
                <args>
                    <module>Dax_Eavmgmt</module>
                    <frontName>eavmgmt</frontName>
                </args>
            </Eavmgmt>
        </routers>
         <layout>
            <updates>
                <eavmgmt>
                    <file>eavmgmt.xml</file>
                </eavmgmt>
            </updates>
        </layout>
    </admin>
</config>